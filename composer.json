{
  "name": "inmarelibero/gitignore-checker",
  "description": "A PHP library to check if a path is ignored by GIT",
  "type": "library",
  "keywords": [
    "Git",
    "Gitignore",
    "VCS"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "Inmarelibero\\GitIgnoreChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inmarelibero\\GitIgnoreChecker\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Emanuele Gaspari",
      "email": "inmarelibero@gmail.com",
      "role": "Developer"
    }
  ],
  "config": {
      "bin-dir": "bin"
  },
  "scripts": {
      "test": "php ./bin/phpunit -c ."
  },
  "homepage": "https://traintosymfony.com"
}
